{
  "expo": {
    "name": "Tcc",
    "slug": "Tcc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sunflower.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sunflower.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.henriqueelimaa.Tcc",
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sunflower.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "73fe3f27-a1a1-4692-8238-d177043f620b"
      }
    },
    "owner": "henriqueelimaa"
  }
}
